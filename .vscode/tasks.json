{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build query-test-2",
            "type": "process",
            "command": "dotnet",
            "args": [
                "build",
                "${workspaceFolder}/query-test-2/query-test-2.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary" 
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "build migrator2",
            "type": "process",
            "command": "dotnet",
            "args": [
                "build",
                "${workspaceFolder}/migrator2/migrator2.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "build term",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/term/term.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "clean term",
            "command": "dotnet",
            "type": "process",
            "args": [
                "clean",
                "${workspaceFolder}/term/term.csproj",
            ],
            "problemMatcher": "$msCompile",
            "group": "none"
        }
    ]
}