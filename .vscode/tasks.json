{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build query-test",
            "type": "process",
            "command": "dotnet",
            "args": [
                "build",
                "${workspaceFolder}/query-test/query-test.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary" 
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "build migrator",
            "type": "process",
            "command": "dotnet",
            "args": [
                "build",
                "${workspaceFolder}/Migrator/Migrator.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "build term",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/term/term.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "clean term",
            "command": "dotnet",
            "type": "process",
            "args": [
                "clean",
                "${workspaceFolder}/term/term.csproj",
            ],
            "problemMatcher": "$msCompile",
            "group": "none"
        },
        {
            "label": "build upload-inventory",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/upload-inventory/upload-inventory.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        }
    ]
}